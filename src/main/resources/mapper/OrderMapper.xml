<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="orders">
    <select id="getOrderId" resultType="int">
        SELECT
            seq_order.NEXTVAL
        FROM
            DUAL
    </select>

</mapper>